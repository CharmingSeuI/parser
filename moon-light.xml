                        <연계항목 type="이미지" 연계시작="ITKC_MO_1237A_B137_312D" 연계종료="ITKC_MO_1237A_B137_313A"/>
                    </연계정보>
                </레벨4>
                <레벨4 type="최종정보" id="ITKC_MO_1237A_0030_010_0320" 자료생성일="2020-10-15" DCI="ITKC_MO_1237A_0030_010_0320_2020_B137_XML">
                    <메타정보>
                        <제목정보>
                            <제목>酬退隱師 <원주>五首</원주></제목>
                        </제목정보>
                        <저자정보 type="필자">
                            <저자>
                                <한글성명>조성가</한글성명>
                                <한자성명>趙性家</한자성명>
                            </저자>
                        </저자정보>
