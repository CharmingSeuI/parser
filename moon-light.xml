                        </제목정보>
                        <저자정보 type="필자">
                            <저자>
                                <한글성명>조성가</한글성명>
                                <한자성명>趙性家</한자성명>
                            </저자>
                        </저자정보>
                        <간행정보>
                            <집수번호>b137</집수번호>
                        </간행정보>
                        <분류정보>
                            <분류항목 type="문체분류">
                                <분류내용 type="대분류">傳記類</분류내용>
                                <분류내용 type="중분류">碑誌類</분류내용>
                            </분류항목>
                        </분류정보>
