                <레벨4 type="최종정보" id="ITKC_MO_1237A_0160_010_0080" 자료생성일="2020-10-15" DCI="ITKC_MO_1237A_0160_010_0080_2020_B137_XML">
                    <메타정보>
                        <제목정보>
                            <제목>贈童蒙敎官趙公墓碣銘 <원주>幷序</원주></제목>
                        </제목정보>
                        <저자정보 type="필자">
                            <저자>
                                <한글성명>조성가</한글성명>
                                <한자성명>趙性家</한자성명>
                            </저자>
                        </저자정보>
                        <간행정보>
                            <집수번호>b137</집수번호>
                        </간행정보>
                        <분류정보>
                            <분류항목 type="문체분류">
                                <분류내용 type="대분류">傳記類</분류내용>
                                <분류내용 type="중분류">碑誌類</분류내용>
                            </분류항목>
                        </분류정보>
